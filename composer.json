{
    "name": "sweetchuck/cdd",
    "description": "Circular dependency detector",
    "license": "GPL-3.0-or-later",
    "config": {
        "bin-dir": "bin",
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.3"
    },
    "require-dev": {
        "ext-json": "*",
        "phpmd/phpmd": "^2.9",
        "phpunit/phpunit": "^8.0",
        "squizlabs/php_codesniffer": "^3.3",
        "sweetchuck/git-hooks": "2.x-dev",
        "sweetchuck/robo-git": "1.x-dev",
        "sweetchuck/robo-phpcs": "1.x-dev",
        "sweetchuck/robo-phpmd": "1.x-dev"
    },
    "autoload": {
        "psr-4": {
            "Sweetchuck\\cdd\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sweetchuck\\cdd\\Tests\\": "src-dev/Tests/"
        }
    },
    "scripts": {},
    "scripts-descriptions": {}
}
